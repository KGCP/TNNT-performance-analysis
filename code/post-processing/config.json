		"gs18": [
			"spacy_lg_model",
            "spacy_md_model",
            "spacy_sm_model",
            "stanza_model",
            "allennlp-finegrained-ner",
            "deeppavlov_onto_bert_model",
            "deeppavlov_onto_model",
            "flair_fast_ontonotes_model",
            "flair_ontonotes_model"
		],
        "sc7": [ "stanford_class7_model" ],
        "nl5": [ "nltk_model" ],
        "ss4": [
        	"allennlp_ner",
            "deeppavlov_conll2003_bert_model",
            "deeppavlov_conll2003_model",
            "flair_fast_model",
            "flair_model",
            "flair_pooled_model",
            "bert_model",
            "stanford_class4_model"
        ],
        "bs3": [ "polyglot_model", "stanford_class3_model" ]
	},
	"mappings": {
		"tnnt:Person"      : [ "gs18:PERSON"  , "sc7:PERSON"      , "nl5:PERSON"      , "ss4:PER"  , "ss4:PERSON"      , "bs3:PER", "bs3:PERSON"       ],
		"tnnt:Organisation": [ "gs18:ORG"     , "sc7:ORGANIZATION", "nl5:ORGANIZATION", "ss4:ORG"  , "ss4:ORGANIZATION", "bs3:ORG", "bs3:ORGANIZATION" ],
		"tnnt:Location"    : [ "gs18:LOC"     ,                     "nl5:LOCATION" ],
		"sc7:LOCATION"     : [ "gs18:LOC"     , "ss4:LOC"         , "ss4:LOCATION"    , "bs3:LOC"  , "bs3:LOCATION"    , "gs18:FAC"     , "gs18:GPE" ],
		
		"tnnt:Money"       : [ "gs18:MONEY"   , "sc7:MONEY"   ],
		"tnnt:Percent"     : [ "gs18:PERCENT" , "sc7:PERCENT" ],
		"tnnt:Date"        : [ "gs18:DATE"    , "sc7:DATE"    ],
		"tnnt:Time"        : [ "gs18:TIME"    , "sc7:TIME"    ],
		
		"tnnt:FAC"         : [ "gs18:FAC"             ],
		"nl5:FACILITY"     : [ "gs18:ORG", "gs18:FAC" ],
		"tnnt:GPE"         : [ "gs18:GPE", "nl5:GPE"  ],
		
		"tnnt:NORP"        : [ "gs18:NORP"     ],
		"tnnt:Product"     : [ "gs18:PRODUCT"  ],
		"tnnt:Event"       : [ "gs18:EVENT"    ],
		"tnnt:Law"         : [ "gs18:LAW"      ],
		"tnnt:Language"    : [ "gs18:LANGUAGE" ],
		"ss4:MISC"         : [ "gs18:NORP", "gs18:PRODUCT", "gs18:EVENT", "gs18:LAW", "gs18:LANGUAGE" ],
				
		"tnnt:WorkOfArt"   : [ "gs18:WORK_OF_ART" ],
		"tnnt:Quantity"    : [ "gs18:QUANTITY"    ],
		"tnnt:Cardinal"    : [ "gs18:CARDINAL"    ],
		"tnnt:Ordinal"     : [ "gs18:ORDINAL"     ]
